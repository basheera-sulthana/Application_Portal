  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.1.3",
    "bootstrap": "^5.2.3",
    "bootstrap-container": "^1.0.0",
    "json-server": "^0.17.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-bootstrap-buttons": "^1.0.0",
    "react-bootstrap-dropdown": "^0.3.0",
    "react-bootstrap-form": "^0.1.4-beta6",
    "react-bootstrap-navbar": "^1.1.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "redux": "^4.2.1",
    "sweetalert2": "^11.7.10",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "webservice": "json-server --watch db.json --id --port 8080"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-jest": "^29.7.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "enzyme-to-json": "^3.6.2",
    "jest": "^27.5.1"
  },

  "jest":
  {
    "collectCoverageForm" : [
      "src/**/*.{js,jsx,ts,tsx}",

      "!<rootDir>/node_modules/",

      "!<rootDir>/path/to/dir/"
    ],

    "coverageThreshold":{

      "global":{

        "branches":90,

        "functions" :90,

        "lines" : 90,

        "statements" : 90
      }
    },

    "moduleNameMapper":{
      "axios" : "axios/dist/node/axios.cjs"
    }
  }
}
